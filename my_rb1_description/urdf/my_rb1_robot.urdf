<?xml version="1.0" ?>

<robot name="rb1">
    <gazebo reference="right_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="left_wheel">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="front_caster">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Grey</material>
    </gazebo>
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="joint_base_link_laser">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="front_laser">
        <material>Gazebo/Black</material>
    </gazebo>
    
    <!--diff drive plugin-->
    <gazebo>
        <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
            <alwaysOn>true</alwaysOn>
            <updateRate>20</updateRate>
            <leftJoint>joint_base_link_left_wheel</leftJoint>
            <rightJoint>joint_base_link_right_wheel</rightJoint>
            <wheelSeparation>0.34</wheelSeparation>
            <wheelDiameter>0.05</wheelDiameter>
            <torque>10</torque>
            <commandTopic>cmd_vel</commandTopic>
            <odometryTopic>odom</odometryTopic>
            <odometryFrame>odom</odometryFrame>
            <robotBaseFrame>base_link</robotBaseFrame>
        </plugin>
    </gazebo>

    <!--scan laser plugin-->
    <gazebo reference="front_laser">
    <sensor type="ray" name="head_hokuyo_sensor">
        <pose>0 0 0 0 0 0</pose>
        <visualize>false</visualize>
        <update_rate>20</update_rate>
        <ray>
            <scan>
                <horizontal>
                    <samples>720</samples>
                    <resolution>1</resolution>
                    <min_angle>-1.570796</min_angle>
                    <max_angle>1.570796</max_angle>
                </horizontal>
            </scan>
            <range>
                <min>0.2</min>
                <max>10.0</max>
                <resolution>0.01</resolution>
            </range>
            <noise>
                <type>gaussian</type>
                <mean>0.0</mean>
                <stddev>0.01</stddev>
            </noise>
        </ray>
        <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
            <topicName>/laser/scan</topicName>
            <frameName>sensor_laser</frameName>
        </plugin>
    </sensor>
</gazebo>
    
    <!-- Link - base_link-->
    <link name="base_link">

        <collision>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
        </collision>
        <visual>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>
    
    <!-- Base footprint -->
    <joint name="joint_base_link_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0.0 -0.15" />
        <child link="base_footprint" />
        <parent link="base_link" />
        <joint_properties damping="0" friction="0" />
    </joint>
   
    <link name="base_footprint">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="25" />
            <inertia ixx="0.578" ixy="0" ixz="0" iyy="0.578" iyz="0" izz="0.781" />
        </inertial>
    </link>
    
    <!-- Laser scan -->
    <joint name="joint_base_link_laser" type="fixed">
        <origin rpy="3.141 0 0" xyz="0.25 0.0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.5" />
            <inertia ixx="0.000252666666667" ixy="0" ixz="0" iyy="0.000252666666667" iyz="0" izz="0.0005"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.035" length="0.04"/>
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.035" length="0.04"/>
            </geometry>
        </collision>
    </link>

    <!-- Joint - base_link / left wheel -->
    <joint name="joint_base_link_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - left wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 -0.05 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 -0.05 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
            <material name="Black">
                <color rgba="0.1 0.1 0.1 1" />
            </material>
        </visual>
    </link>
   
     <!-- Joint - base_link / right wheel -->
    <joint name="joint_base_link_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - right wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>

        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0.05 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
            <material name="Black">
                <color rgba="0.1 0.1 0.1 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - back caster wheel -->
    <joint name="joint_base_link_bcaster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
        <joint_properties damping="0" friction="0" />
    </joint>
    <!-- Link - caster wheel -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.0124" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.0124" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - front caster wheel -->
    <joint name="joint_base_link_fcaster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
        <joint_properties damping="0" friction="0" />
    </joint>
    <!-- Link - caster wheel -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.0124" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.0124" />
            <geometry>
                <sphere radius="0.0125" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>
</robot>
